<div>
  <main>
    {{#with person}}
    <section>
      {{> title big=true data=name}}
      <hr>
      <div>вік - {{age}}</div>
      <hr>
      <div>стать - {{gender}}</div>
      <hr>
      <div>
        адреса -
        <div>
          <address>
            {{#each address}}
            {{this}}
            {{/each}}
          </address>
        </div>
      </div>
      <hr>
      <div>
        <b>освіта</b>
        <ul class="mt-2">
          {{#each education}}
          <li>
            <div>{{degree}}</div>
            <div>{{major}}</div>
            <div>{{university}}</div>
            <time>{{graduationYear}}</time>
          </li>
          {{/each}}
        </ul>
      </div>
      <hr>
      <div>
        <b>компанія</b>
        <ul class="mt-2">
          {{#each workExperience}}
          <li>
            <div class="mb-3">{{company}}</div>
            {{> title data=title}}
            <div class="mb-2">
              <time>час роботи -
                {{startDate}} -
                {{#if endDate}}
                {{endDate}}
                {{else}}
                (зараз працюю)
                {{/if}}
              </time>
            </div>
            <div>
              <b>опис</b>
              <ul class="mt-2 mb-2">
                {{#each responsibilities}}
                <li>
                  {{this}}
                </li>
                {{/each}}
              </ul>
            </div>
            <div>
              рік заснування -
              {{year_founded}}
            </div>
            <div>
              сфера -
              {{industry}}
            </div>
            <hr>
            <div>
              <b>працівники</b>
              <ul class="mt-2">
                {{#each employees}}
                <li>
                  <div>name - {{name}}</div>
                  <div>position - {{position}}</div>
                  <div>departament - {{department}}</div>
                  <hr>
                  <div>
                    <b>projects</b>
                    <ul class="mt-2">
                      {{#each projects}}
                      <li>
                        <div>name - {{name}}</div>
                        <div>description - {{description}}</div>
                        <div>status - {{status}}</div>
                        <hr>
                        <div>
                          <b>teams</b>
                          <ul class="mt-2">
                            {{#each teams}}
                            <li>
                              <div>{{team_name}}</div>
                              {{#with team_leader}}
                              <div>
                                <div>{{name}}</div>
                                <div>{{title}}</div>
                                {{> link blank=true href=email text=email noLine=true}}
                              </div>
                              {{/with}}
                              <hr>
                              <div>
                                <b>team members</b>
                                <ul class="mt-2">
                                  {{#each team_members}}
                                  <li>
                                    <div>{{name}}</div>
                                    <div>{{title}}</div>
                                    {{> link blank=true href=email text=email
                                    noLine=true}}
                                    <hr>
                                    <div>
                                      <b>skills</b>
                                      <ul class="mt-2">
                                        {{#each skills}}
                                        <li>
                                          {{this}}
                                        </li>
                                        {{/each}}
                                      </ul>
                                    </div>
                                    <hr>
                                    <div>
                                      <b>projects</b>
                                      <ul class="mt-2">
                                        {{#each projects}}
                                        <li>
                                          <div>{{name}}</div>
                                          <div>{{description}}</div>
                                          <div>
                                            <ul>
                                              {{#each technologies}}
                                              <li>
                                                {{this}}
                                              </li>
                                              {{/each}}
                                            </ul>
                                          </div>
                                          <hr>
                                          <div>
                                            <b>team members</b>
                                            <ul class="mt-2">
                                              {{#each team_members}}
                                              <li>
                                                <div>{{name}}</div>
                                                <div>{{title}}</div>
                                              </li>
                                              {{/each}}
                                            </ul>
                                          </div>
                                        </li>
                                        {{/each}}
                                      </ul>
                                    </div>
                                  </li>
                                  {{/each}}
                                </ul>
                              </div>
                            </li>
                            {{/each}}
                          </ul>
                        </div>
                      </li>
                      {{/each}}
                    </ul>
                  </div>
                </li>
                {{/each}}
              </ul>
            </div>
          </li>
          {{/each}}
        </ul>
      </div>
    </section>
    {{/with}}
  </main>
</div>