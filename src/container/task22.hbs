<main>
    {{#with page_header}}
    <header>

        <h1 class='text-center'>{{title}}</h1>

        <div>
            <p class='text-center text-uppercase'>{{description}}</p>
        </div>

    </header>
    {{/with}}

    {{#with section}}
    <section>

        {{#with about_us}}
        <div class="card card-body">
            {{#with title_list}}
            <div class="text-end">

                <div class="display-6">{{main}}</div>

                <h2>{{sub}}</h2>

            </div>
            {{/with}}

            <div>
                <button class="btn btn-outline-info">{{button.text}}</button>
            </div>

        </div>
        {{/with}}

        <div class="card card-body">
            {{#with advantage}}

            <div class="display-5 text-muted">{{title}}</div>

            {{#with efficiency}}
            <div class="card card-body">

                <div class="display-4 text-muted">{{title}}</div>

                <h6>{{sub_title}}</h6>

                <div>
                    <p>{{description}}</p>
                </div>

                <div>
                    <button class="btn btn-outline-warning">{{button}}</button>
                </div>

            </div>
            {{/with}}

            {{#with innovation}}
            <div class="card card-body">

                <div class="display-4 text-center text-muted">{{title}}</div>

                <h6 class="text-center">{{sub_title}}</h6>

                <div class='text-center'>
                    <p>{{description}}</p>
                </div>

                <div>
                    <button class="btn btn-outline-warning">{{button}}</button>
                </div>

            </div>
            {{/with}}

            {{#with collaboration}}
            <div class="card card-body">

                <div class="display-4 text-end text-muted">{{title}}</div>

                <h6 class="text-end">{{sub_title}}</h6>

                <div class='text-end'>
                    <p>{{description}}</p>
                </div>

                <div>
                    <button class="btn btn-outline-warning">{{button}}</button>
                </div>

            </div>
            {{/with}}


            {{/with}}
        </div>

        {{#with agreement}}
        <div class="card card-body">

            <div class="display-5">{{title}}</div>

            {{#with info}}
            <div class="card card-body">

                <h5>{{sub_title}}</h5>

                {{#with detail}}
                <div class="card card-body">

                    <div>
                        <p>{{text}}</p>
                    </div>

                    <div>
                        <button class="btn btn-success">{{button}}</button>
                    </div>

                </div>
                {{/with}}

            </div>
            {{/with}}

        </div>
        {{/with}}

    </section>
    {{/with}}

</main>