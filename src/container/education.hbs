<div>
   {{#with main}}
   <main>

      <section class="mb-5">

         {{> title big=true data="My education"}}

         <ul>
            {{#each educations}}
            <li>{{name}}
               {{#unless isEnd}}
               (Ще в процесі)
               {{/unless}}
            </li>
            {{/each}}
         </ul>

      </section>

      <section>

         {{> title big=false data="My certificates"}}

         <ul>
            {{#each certificates}}
            <li>{{name}} ({{id}})</li>
            {{/each}}
         </ul>

      </section>

   </main>
   {{/with}}
</div>